{% if content.field_link.0 != "" %}
<section class="about-style-4 z-sec">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="section_title">
                    <h2>{{content.field_title.0}}</h2>
                </div>    
                <div class="content-descriptions">
                    <p>{{content.field_formatted_description.0}}</p>
                </div>
                <div class="exp-list" >
                    <div>
                        <h2 style="background-image:url('{{file_url(content.field_image_2['#items'].entity.uri.value)}}');background-repeat:no-repeat;background-size:cover;background-position: bottom;">{{content.field_text.0}}</h2>
                        <h4>{{content.field_text_2.0}}</h4>
                    </div>
                    <div class="list1 lists-block ">
                        <div class="list-wrapper">
                            <ul class="list-unstyled">
                            {% for key,value in content.field_list['#items']%}
                                <li>
                                    <div class="svg-wrapper">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="200" height="150" viewBox="0 0 200 150">
                                            <path  data-name="Path 40733" d="M374.334,167.3c-35.362,36.588-70.75,73-106.152,109.59l-54.823-47.166-18.5,22.941,64.975,55.9,10.152,8.734,9.361-9.665c38.482-39.814,76.984-79.391,115.511-119.253L374.328,167.3Z" transform="translate(-194.86 -167.301)" fill="#1b1f29"/>
                                        </svg>
                                    </div>
                                    <p>{{content.field_list[key]}}</p>
                                </li>
                                {% endfor %} 
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="get-quate-btn home4-btn">
                    <a class=" btn-custom btn-lg" href="{{content.field_link.0['#url']}}">
                        {{content.field_link.0['#title']}}
                        <svg xmlns="http://www.w3.org/2000/svg" width="150" height="44" viewBox="0 0 150 44">
                        <path data-name="Path 40738" d="M44,24.431,23.919,0,0,24.431H19.489V150h7.087V24.431Z" transform="translate(150) rotate(90)" fill="#1b1f29" fill-rule="evenodd"></path>
                        </svg>
                    </a>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="image-wrapper">
                    <img src="{{file_url(content.field_image['#items'].entity.uri.value)}}" alt="{{file_url(content.field_image['#items'].alt)}}">
                </div>
            </div>
        </div>    
    </div>
</section>
