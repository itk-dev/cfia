{% set paragraph_parent = paragraph.getParentEntity() %}
{% set variant = paragraph_parent.type.value[0].target_id %}
{% if variant == "home_slider_style_1" %}
    <div class="item">
        <div class="home-slide-wrapper">
            <div class="slide-image-wrapper">
                <img class="slider-image" src="{{file_url(content.field_image['#items'].entity.uri.value)}}" alt="{{content.field_image['#items'].alt}}">
            </div>
            <div class="container">
                <div class="home-slide">
                    <div class="slider-title">
                        
                        <h1>{{content.field_title.0}}</h1>
                    </div>    
                    <div class="get-quate-btn">
                        <a class=" btn-custom btn-lg" href="{{content.field_link.0['#url']}}">
                            {{content.field_link.0['#title']}}
                            <svg xmlns="http://www.w3.org/2000/svg" width="150" height="44" viewBox="0 0 150 44">
                            <path data-name="Path 40738" d="M44,24.431,23.919,0,0,24.431H19.489V150h7.087V24.431Z" transform="translate(150) rotate(90)" fill="#1b1f29" fill-rule="evenodd"></path>
                            </svg>
                        </a>
                    </div>
                </div>    
            </div>    
        </div>
    </div>
{# code #}
