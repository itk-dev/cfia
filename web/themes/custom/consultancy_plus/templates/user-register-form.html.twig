{% set base_url = url('<front>')  %}
{% if register_banner_title %}
{% if register_banner_image_url %}
<section class="page-banner" style="background-image: url('{{register_banner_image_url}}');background-repeat: no-repeat;background-size: cover;">
{% else %}
<section class="page-banner" style="background-image: url('{{base_url}}sites/default/files/default_images/SubPage-Banner.jpeg');background-repeat: no-repeat;background-size: cover;">
{% endif %}
  <div class="container">
      <div class="heading">
          <h1>{{register_banner_title}}</h1>
          <ol class="breadcrumb">
              <li class="breadcrumb-item"><a href="{{base_url}}">Home</a></li>
              <li class="breadcrumb-item active" aria-current="page">{{register_banner_title}}</li>
          </ol>
      </div>
  </div>
</section>
{% endif %}
<section class="register common-pages z-sec">
  <div class="bg-white">
  </div>
  <div class="bg_primary">
    <div class="container">
        <div class="row">
            <div class="col-lg-5 col-md-6 col-12">
              <div class="pages-title">
                {{register_bottom_content|raw}} 
                    <div class="get-quate-btn">
                      <a class=" btn-custom btn-lg" href="{{register_button_url}}">
                          {{register_button_title}}
                          <svg xmlns="http://www.w3.org/2000/svg" width="150" height="44" viewBox="0 0 150 44">
                          <path data-name="Path 40738" d="M44,24.431,23.919,0,0,24.431H19.489V150h7.087V24.431Z" transform="translate(150) rotate(90)" fill="#1b1f29" fill-rule="evenodd"></path>
                          </svg>
                      </a>
                    </div>
                </div>
            </div>
            <div class="col-lg-7 col-md-6"> 
                <div class="register-form common-page-form">
                    <div class="register-inner">
                        {% if register_title %} 
                          <h2>{{register_title}}</h2> 
                        {% endif %} 
                        <div class="form-group">
                          {{ form.account.mail }}
                          {% if register_email_text %}
                          
                          <span>{{register_email_text}}</span>
                          {% endif %}
                        </div>
                        <div class="form-group">
                            {{ form.account.name }}
                          {% if register_name_text %}
                          <span>{{register_name_text}}</span>
                          {% endif %}
                        </div>
                        <div class="register-btn">
                            <div class="quate-btn-rect home1-btn">{{ form.actions }}</div>
                            <h6>{{register_content|raw}}</h6>
                        </div>
                    </div>
                    {{ form.form_build_id }}
                    {{ form.form_id }}
                </div>
            </div>
        </div>
    </div>
  </div>  
</section>