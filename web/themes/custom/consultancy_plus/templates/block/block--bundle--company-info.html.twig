      </div>  
    {% endif %}
    <div class="company-info">
      {{content.field_address.0}}
      <div class="mail-ph">
        <a href="mailto:{{content.field_email.0}}">{{content.field_email.0}}</a>
        <a href="tel:{{content.field_contact_number.0}}">{{content.field_contact_number.0}}</a>
      </div>
      {{content.field_form.0}}
    </div>  
  {% endblock %}
</nav>