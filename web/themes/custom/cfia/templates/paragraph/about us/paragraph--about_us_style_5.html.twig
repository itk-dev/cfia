{% if content.field_image['#items'] != "" %}
<section class="about-style-5 dt-section">
    <div class="container">
            <div class="image-wrapper">
                <img src="{{file_url(content.field_image['#items'].entity.uri.value)}}" alt="{{file_url(content.field_image['#items'].alt)}}">
            </div>
        <div class="row">
            <div class="col-lg-6">
                <div class="tagline-title home5">
                    <span>{{content.field_tagline.0}}</span>
                    <h2>{{content.field_title.0}}</h2>
                </div>
                <div class="content-descriptions">
                    <p>{{content.field_formatted_description.0}}</p>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="about-list ">
                    {{content.field_widget}}
                </div>
            </div>
        </div>
    </div>
</section>
{% else %}
<section class="about-style-5 z-sec ">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <div class="tagline-title home5">
                    <span>{{content.field_tagline.0}}</span>
                    <h2>{{content.field_title.0}}</h2>
                </div>
                <div class="content-descriptions">
                    <p>{{content.field_formatted_description.0}}</p>
                </div>
            </div>
            <div class="col-lg-6">
                <div class="about-list">
                    {{content.field_widget}}
                </div>
            </div>
        </div>
    </div>
</section>
{% endif %}
